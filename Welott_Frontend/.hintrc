{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-ms-touch-action",
          "-moz-backface-visibility"
        ]
      }
    ],
    "disown-opener": "off",
    "meta-viewport": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off",
        "document-title": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "listitem": "off"
      }
    ],
    "apple-touch-icons": "off",
    "typescript-config/consistent-casing": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "form[is]"
        ]
      }
    ],
    "meta-charset-utf-8": "off"
  }
}